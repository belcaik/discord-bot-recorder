{
  "name": "discord-bot-recorder",
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "build": "tsc --checkJs --allowJs --outDir ./dist --rootDir ./src ",
    "start": "dotenv -e .env.local node ./dist/index.js",
    "test": "jest",
    "watch": "tsc -w",
    "dev:local": "dotenv -e .env.local ts-node-dev ./src/index.ts --respawn --transpile-only --poll"
  },
  "devDependencies": {
    "@types/libsodium-wrappers": "^0",
    "@types/eslint": "^8",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@yarnpkg/pnpify": "^4.0.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@discordjs/voice": "^0.16.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.2",
    "jest": "^29.7.0",
    "libsodium-wrappers": "^0.7.13",
    "prism-media": "^1.3.5"
  }
}
